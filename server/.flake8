[flake8]
ignore=D100,D104,D301
max-line-length=99
exclude=tests
inline-quotes="
import-order-style=appnexus
application-package-names=athenian.api
per-file-ignores=
    setup.py:D
    athenian/api/models/__init__.py:F401
    athenian/api/models/*.py:D400,D401,D403
    athenian/api/models/db/github/__init__.py:D101