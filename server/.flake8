[flake8]
ignore=D100,D104,D301,W503,W504
max-line-length=99
inline-quotes="
import-order-style=appnexus
application-package-names=athenian.api,tests
per-file-ignores=
    setup.py:D
    tests/*:D
    athenian/api/models/web/__init__.py:F401
    athenian/api/models/*.py:D400,D401,D403
    athenian/api/models/metadata/github/__init__.py:D101
    athenian/api/models/state/versions/*.py:D103,D400
